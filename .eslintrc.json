{
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "editorconfig",
    "import"
  ],
  "extends": [
    "airbnb-base",
    "plugin:editorconfig/all"
  ],
  "settings": {
    "import/resolver": {
      "alias": {
        "map": [
          [
            "@",
            "./src"
          ]
        ]
      }
    }
  }
}
